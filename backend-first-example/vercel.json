{
	"version": 2,
	"builds": [
		{
			"src": "index.js",
			"use": "@vercel/node"
		}
	],
	"routes": [
		{
			"src": "/api/(.*)",
			"dest": "/index.js"
		},
		{
			"src": "/(.*)",
			"dest": "/dist/$1"
		},
		{
			"handle": "filesystem"
		},
		{
			"src": "/(.*)",
			"dest": "/dist/index.html"
		}
	]
}
